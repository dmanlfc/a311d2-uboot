		};
	};

	i2c4_pins2:i2c4_pins2 {
		mux {
			groups = "i2c4_sda_h",
				"i2c4_sck_h";
			function = "i2c4";
			drive-strength = <3>;
			bias-disable;
		};
	};

	i2c5_pins1:i2c5_pins1 {
		mux {
			groups = "i2c5_sda",
				"i2c5_sck";
			function = "i2c5";
			drive-strength = <3>;
			bias-disable;
		};
	};

	i2c0_ao_pins1:i2c0_ao_pins1 {
		mux {
			groups = "i2c0_ao_sda_d",
				"i2c0_ao_sck_d";
			function = "i2c0_ao";
			drive-strength = <3>;
			bias-disable;
		};
	};

	i2c0_ao_pins2:i2c0_ao_pins2 {
